    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^3.1.0",
    "jest": "^26.2.2",
    "nock": "^9.0.2",
    "nodemon": "^2.0.12",
    "npm-run-all": "^3.1.1",
