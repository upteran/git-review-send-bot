  "description": "Telegram bot to send and set links to chat user",
  "main": "index.ts",
  "scripts": {
    "test": "jest --testPathPattern .*\\.test\\.ts",
    "build": "tsc --project tsconfig.json",
    "test:watch": "yarn test --watch",
    "start": "node dist/index.js",
    "start:dev": "nodemon -x ts-node . -e ts,json",
    "dev": "npm-run-all --parallel start:dev test:watch"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
