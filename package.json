  "bugs": {
    "url": "https://github.com/upteran/git-review-send-bot/issues"
  },
  "homepage": "https://github.com/upteran/git-review-send-bot#readme",
  "dependencies": {
    "@types/got": "^9.6.12",
    "@types/node": "^14.17.18",
    "dayjs": "^1.10.7",
    "firebase": "^9.1.1",
    "got": "^11.5.1",
    "nanoid": "^3.1.28",
    "telegraf": "^3.39.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "typescript-eslint": "0.0.1-alpha.0",
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
